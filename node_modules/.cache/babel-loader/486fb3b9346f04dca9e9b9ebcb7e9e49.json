jD,IAAA,KAAK,EAAC;AAA2C,GAAvF,EAA4E,IAA5E,CAEE,EADF,mBAAA,CAAqF,QAArF,EAAqF;AAA7E,IAAA,IAAI,EAAC,QAAwE;AAA9D,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,MAAA,IAAA,QAAA,CAAA,MAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAAyD;AAA/C,IAAA,KAAK,EAAC;AAAyC,GAArF,EAA0E,IAA1E,CACE,CAHN,CAIE,CALN,CAMI,CA/BN,C","sourcesContent":["<template>\n  <div class=\"body\">\n      <div class=\"progressbar-container\">\n          <ProgressBar :percentage=percentage />\n      </div>\n      <div class=\"question-title\">\n          {{page[0].flow_txt}}\n      </div>\n    <div class=\"question-container\">\n        <div class=\"single-select\">\n            <div class=\"title\">\n                Q. {{ page[0].question_txt }}\n            </div>\n            <div class=\"answer-box\">\n                <button type=\"button\" @click=\"setAnswer(page[0].column, i)\" v-for=\"(ans, i) in page[0].answer_options\" :key=\"i\" :class=\"{'selected': answer_1[i] == true}\" class=\"btn-primary-answer btn-50-ans\">{{ans.content}}</button>\n            </div>\n        </div>\n        <div class=\"single-select\">\n            <div class=\"title\">\n                Q. {{ page[1].question_txt }}\n            </div>\n            <div class=\"answer-box\">\n                <button type=\"button\" @click=\"setAnswer(page[1].column, i)\" v-for=\"(ans, i) in page[1].answer_options\" :key=\"i\" :class=\"{'selected': answer_2[i] == true}\" class=\"btn-primary-answer btn-50-ans\">{{ans.content}}</button>\n            </div>\n        </div>\n    </div>\n    <div class=\"button-container\">\n        <div class=\"buttons\">\n            <button type=\"button\" @click=\"goBack\" class=\"btn-secondary btn-55 prev-btn\">이전</button>\n            <button type=\"button\" @click=\"goNext\" class=\"btn-primary btn-55 next-btn\">다음</button>\n        </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProgressBar from '@/components/ProgressBar.vue';\nimport { survey, page_info } from '../../Survey'\nimport axios from 'axios'\n\nexport default {\n    data() {\n        return {\n            paging: page_info,\n            survey: survey,\n            page: survey.page3,\n            answer_1: [false,false,false,false,false,false,false,false,false,false],\n            answer_2: [false,false,false,false,false,false,false,false,false,false,false],\n            answer1: '', \n            answer2: '',\n            percentage: '',\n        }\n    },\n    components: {\n        ProgressBar,\n    },\n    methods: {\n      setAnswer(column, index) {\n        if (column == this.page[0].column) {\n          this.answer_1[index] = true\n          this.answer1 = this.page[0].answer_options[index].id\n          for (let idx in this.answer_1) {\n            if (idx != index) this.answer_1[idx] = false\n          }\n        }\n        else if (column == this.page[1].column) {\n          this.answer_2[index] = true\n          this.answer2 = this.page[1].answer_options[index].id\n          for (let idx in this.answer_2) {\n            if (idx != index) this.answer_2[idx] = false\n          }\n        }\n      },\n        async goBack() {\n            let answers = {\n              usercode: this.$store.state.usercode,\n              [this.page[0].column] : Number(this.answer1),\n              [this.page[1].column] : Number(this.answer2),\n            }\n            await axios.post('/breastTest/save/3', answers)\n            .then((result) => {\n              console.log(result.data)\n              this.$router.push('/survey/2')\n            })\n            .catch(console.log);\n        },\n        async goNext() {\n          console.log(Number(this.answer1));\n          console.log(Number(this.answer2));\n\n            let answers = {\n              usercode: this.$store.state.usercode,\n              [this.page[0].column] : Number(this.answer1),\n              [this.page[1].column] : Number(this.answer2),\n            }\n            await axios.post('/breastTest/save/3', answers)\n            .then((result) => {\n              console.log(result.data);\n              this.$router.push('/survey/4')\n            })\n            .catch(console.log);\n        },\n        async fetchAnswerInfo() {\n          // 이전에 저장된 답변 정보 불러오기\n          await axios.get('/breastTest/getData/3')\n          .then((result) => {\n            console.log(result.data.record);\n            if (result.data.success) {\n              this.answer1 = result.data.record.BRA_SIZE_CUP;\n              this.answer2 = result.data.record.BRA_SIZE_UB;\n              this.answer_1[this.answer1-1] = true;\n              this.answer_2[this.answer2-1] = true;\n            }\n          }).catch(console.log);\n        },\n        async fetchProgress() {\n          axios.get('/breastTest/getProgress')\n            .then((result) => {\n                console.log(result.data);\n                this.percentage = result.data.Progress;\n            }).catch(console.log);\n        }        \n    },\n    created() {\n        this.fetchAnswerInfo()\n        this.fetchProgress()\n    },\n    mounted() {\n      window.scrollTo(0, 0)\n    },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n    height: 150vh;\n    background-color: $gray;\n    display: flex-start;\n    flex-direction: column;\n    justify-content: center;\n    align-items: flex-start;\n    padding: 100px 100px;\n\n    @media screen and (max-width: $md-breakpoint) {\n        /* padding: 90px 55px;  */\n        height: 1200px;\n        padding: 100px 16px;\n    }\n\n    .question-container {\n        margin: 40px;\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: center;\n    }\n\n    .question-title {\n        @include text-style(16, $primary);\n        margin-top: 8px;\n        display: flex;\n        justify-content: center;\n        align-items: center; \n\n        @media screen and (max-width: $md-breakpoint) {\n            @include text-style(14, $primary);\n            margin-top: 8px;\n        }\n\n        p {\n            text-align: center;\n        }\n    }\n\n    .input-set {\n        margin: 16px;\n        .title {\n            @include text-style(16, $primary);\n            margin-right: 16px;\n        }\n\n        display: flex;\n        align-items: center;\n        justify-content: center;\n\n        @media screen and (max-width: $md-breakpoint) {\n            flex-direction: column;\n            @include text-style(14, $primary);\n\n        }\n    }\n\n    .short-answer {\n        margin: 8px;\n    }\n\n    .button-container {\n        margin: 16px;\n        display: flex;\n        justify-content: center;\n\n        button {\n            margin: 8px;\n            width: 144px;\n            @media screen and (max-width: $md-breakpoint) {\n                margin: 4px;\n                width: 100px;\n            }\n        }\n    }\n\n    .single-select {\n        /* width: 80%; */\n        .title {\n            @include text-style(18, $white);\n            /* margin-right: 16px; */\n            width: 100%;\n            text-align: center;\n            background-color: $blue;\n            color: $white;\n            padding: 8px;\n            border-top-left-radius: 8px;\n            border-top-right-radius: 8px;\n            /* margin-top: 8px; */\n            @media screen and (max-width: $md-breakpoint) {\n              @include text-style(16, $white);          \n            }            \n        }        \n        background-color: $white;\n        display: flex; \n        flex-direction: column; \n        justify-content: center; \n        align-items: center; \n        width: 600px;\n        margin: 32px;\n        border-radius: 8px;\n        box-shadow: 1px 4px 4px 1px $border;\n\n        @media screen and (max-width: $md-breakpoint) {\n          @include text-style(14, $primary);          \n          width: 330px;\n          margin: 16px 8px;\n        }\n    }  \n\n\n  .answer-box {\n      margin: 32px;\n      display: flex;\n      flex-wrap: wrap;\n      justify-content: center;\n      align-items: center;\n      /* width: 100%; */\n\n      button {\n        @include text-style(14);\n        margin: 4px;\n        width: 100px;\n\n        @media screen and (max-width: $md-breakpoint) {\n          width: 60px;\n        }\n      }\n\n      @media screen and (max-width: $md-breakpoint) {\n        margin: 16px 40px;\n      }\n\n  }  \n\n  .selected {\n      /* border: 2px solid $secondary; */\n      background-color: $blue-dark;\n  }\n}\n</style>"],"sourceRoot":""}]}