,CAAC,IAAL,CAAU,CAAV,EAAa,MAAhC,CAApC;AACF;AACD,iBAPG,EAQH,KARG,CAQG,OAAO,CAAC,GARX,CAFgB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWvB,KAxDM;AAyDD,IAAA,aAzDC,2BAyDe;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACd,KAAK,CAAC,GAAN,CAAU,yBAAV,EACH,IADG,CACE,UAAC,MAAD,EAAY;AAChB,kBAAA,OAAO,CAAC,GAAR,CAAY,MAAM,CAAC,IAAnB;AACA,kBAAA,MAAI,CAAC,UAAL,GAAkB,MAAM,CAAC,IAAP,CAAY,QAA9B;AACD,iBAJG,EAKH,KALG,CAKG,OAAO,CAAC,GALX,CADc;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOrB;AAhEM,GAhBI;AAkFb,EAAA,OAlFa,qBAkFH;AACR,SAAK,eAAL;AACA,SAAK,aAAL;AACD,GArFY;AAsFb,EAAA,OAtFa,qBAsFH;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;AACD;AAxFY,CAAf","sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"progressbar-container\">\n      <ProgressBar :percentage=\"percentage\" />\n    </div>\n    <div class=\"question-title\">\n      {{ page[0].flow_txt }}\n    </div>\n    <div class=\"question-container\">\n      <div class=\"single-select\">\n        <div class=\"title\">Q. {{ page[0].question_txt }}</div>\n        <div class=\"answer-box\">\n          <button\n            type=\"button\"\n            @click=\"setAnswer(page[0].column, i)\"\n            v-for=\"(ans, i) in page[0].answer_options\"\n            :key=\"i\"\n            :class=\"{ selected: answer_1[i] == true }\"\n            class=\"btn-primary-answer btn-50-ans\"\n          >\n            {{ ans.content }}\n          </button>\n        </div>\n      </div>\n      <div class=\"single-select\">\n        <div class=\"title\">Q. {{ page[1].question_txt }}</div>\n        <div class=\"answer-box\">\n          <button\n            type=\"button\"\n            @click=\"setAnswer(page[1].column, i)\"\n            v-for=\"(ans, i) in page[1].answer_options\"\n            :key=\"i\"\n            :class=\"{ selected: answer_2[i] == true }\"\n            class=\"btn-primary-answer btn-50-ans\"\n          >\n            {{ ans.content }}\n          </button>\n        </div>\n      </div>\n    </div>\n    <div class=\"button-container\">\n      <div class=\"buttons\">\n        <button type=\"button\" @click=\"goBack\" class=\"btn-secondary btn-55 prev-btn\">이전</button>\n        <button type=\"button\" @click=\"goNext\" class=\"btn-primary btn-55 next-btn\">다음</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProgressBar from \"@/components/ProgressBar.vue\";\nimport { survey, page_info } from \"../../Survey\";\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      paging: page_info,\n      survey: survey,\n      page: survey.page3,\n      answer_1: [false, false, false, false, false, false, false, false, false, false],\n      answer_2: [false, false, false, false, false, false, false, false, false, false, false],\n      answer1: null,\n      answer2: null,\n      percentage: 0,\n    };\n  },\n  components: {\n    ProgressBar,\n  },\n  methods: {\n    setAnswer(column, index) {\n      if (column == this.page[0].column) {\n        this.answer1 = index;\n        for (let idx in this.answer_1) {\n          this.answer_1[idx] = false;\n        }\n        this.answer_1[index] = true;\n      } else if (column == this.page[1].column) {\n        this.answer2 = index;\n        for (let idx in this.answer_2) {\n          this.answer_2[idx] = false;\n        }\n        this.answer_2[index] = true;\n      }\n    },\n    async goBack() {\n      let answers = {\n        PK_ID: this.$store.state.PK_ID,\n        [this.page[0].column]: this.answer1,\n        [this.page[1].column]: this.answer2,\n      };\n      await axios.post(\"/breastTest/save/3\", answers)\n        .then((result) => {\n          console.log(result.data);\n          this.$router.push(\"/survey/2\");\n        })\n        .catch(console.log);\n    },\n    async goNext() {\n      if (this.answer1 === null || this.answer2 === null) {\n        return this.emitter.emit(\"showRedToast\", \"답변을 입력해주세요\");\n      }\n      let answers = {\n        PK_ID: this.$store.state.PK_ID,\n        [this.page[0].column]: this.answer1,\n        [this.page[1].column]: this.answer2,\n      };\n      await axios.post(\"/breastTest/save/3\", answers)\n        .then((result) => {\n          console.log(result.data);\n          this.$router.push(\"/survey/4\");\n        })\n        .catch(console.log);\n    },\n    async fetchAnswerInfo() {\n      // 이전에 저장된 답변 정보 불러오기\n      await axios.get(\"/breastTest/getData/3\")\n        .then((result) => {\n          console.log(result.data.record);\n          if (result.data.success) {\n            this.setAnswer(this.page[0].column, result.data.record[this.page[0].column])\n            this.setAnswer(this.page[1].column, result.data.record[this.page[1].column])\n          }\n        })\n        .catch(console.log);\n    },\n    async fetchProgress() {\n      await axios.get(\"/breastTest/getProgress\")\n        .then((result) => {\n          console.log(result.data);\n          this.percentage = result.data.Progress;\n        })\n        .catch(console.log);\n    },\n  },\n  created() {\n    this.fetchAnswerInfo();\n    this.fetchProgress();\n  },\n  mounted() {\n    window.scrollTo(0, 0);\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n  height: 150vh;\n  background-color: $gray;\n  display: flex-start;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 100px 100px;\n\n  @media screen and (max-width: $md-breakpoint) {\n    /* padding: 90px 55px;  */\n    height: 1200px;\n    padding: 100px 16px;\n  }\n\n  .question-container {\n    margin: 40px;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n  }\n\n  .question-title {\n    @include text-style(16, $primary);\n    margin-top: 8px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n\n    @media screen and (max-width: $md-breakpoint) {\n      @include text-style(14, $primary);\n      margin-top: 8px;\n    }\n\n    p {\n      text-align: center;\n    }\n  }\n\n  .input-set {\n    margin: 16px;\n    .title {\n      @include text-style(16, $primary);\n      margin-right: 16px;\n    }\n\n    display: flex;\n    align-items: center;\n    justify-content: center;\n\n    @media screen and (max-width: $md-breakpoint) {\n      flex-direction: column;\n      @include text-style(14, $primary);\n    }\n  }\n\n  .short-answer {\n    margin: 8px;\n  }\n\n  .button-container {\n    margin: 16px;\n    display: flex;\n    justify-content: center;\n\n    button {\n      margin: 8px;\n      width: 144px;\n      @media screen and (max-width: $md-breakpoint) {\n        margin: 4px;\n        width: 100px;\n      }\n    }\n  }\n\n  .single-select {\n    /* width: 80%; */\n    .title {\n      @include text-style(18, $white);\n      /* margin-right: 16px; */\n      width: 100%;\n      text-align: center;\n      background-color: $blue;\n      color: $white;\n      padding: 8px;\n      border-top-left-radius: 8px;\n      border-top-right-radius: 8px;\n      /* margin-top: 8px; */\n      @media screen and (max-width: $md-breakpoint) {\n        @include text-style(16, $white);\n      }\n    }\n    background-color: $white;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    width: 600px;\n    margin: 32px;\n    border-radius: 8px;\n    box-shadow: 1px 4px 4px 1px $border;\n\n    @media screen and (max-width: $md-breakpoint) {\n      @include text-style(14, $primary);\n      width: 330px;\n      margin: 16px 8px;\n    }\n    @media screen and (max-width: 320px) {\n      flex-direction: column;\n      width: 300px;\n      margin: 16px 8px;\n      @include text-style(14, $primary);\n    }    \n  }\n\n  .answer-box {\n    margin: 32px;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    align-items: center;\n    /* width: 100%; */\n\n    button {\n      @include text-style(14);\n      margin: 4px;\n      width: 100px;\n\n      @media screen and (max-width: $md-breakpoint) {\n        width: 60px;\n      }\n    }\n\n    @media screen and (max-width: $md-breakpoint) {\n      margin: 16px 40px;\n    }\n  }\n\n  .selected {\n    /* border: 2px solid $secondary; */\n    background-color: $blue-dark;\n  }\n}\n</style>\n"],"sourceRoot":""}]}