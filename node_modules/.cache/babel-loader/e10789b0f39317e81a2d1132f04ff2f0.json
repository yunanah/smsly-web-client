aAAX,KAAA,CAAA,O,GAAO,M;;AAAI,GAAvC,E,IAAA,E;;AAAA,G,iBAA4B,KAAA,CAAA,O,GAAgB,CAAhD,CACG,CAHL,CAQM,EAJN,mBAAA,CAGK,IAHL,EAGK,IAHL,EAGK,CAFH,WAEG,EADH,mBAAA,CAAqD,IAArD,EAAqD,IAArD,EAAqD,C,gBAAjD,mBAAA,CAA4C,OAA5C,EAA4C;AAArC,IAAA,IAAI,EAAC,MAAgC;;aAAhB,KAAA,CAAA,Y,GAAY,M;;AAAI,GAA5C,E,IAAA,E;;AAAA,G,iBAA4B,KAAA,CAAA,Y,GAAqB,CAArD,CACG,CAHL,CAIM,CAbR,CA+BI,EAhBJ,mBAAA,CAeM,KAfN,EAeM;AAdJ,IAAA,KAAK,EAAC,iBAcF;AAbJ,IAAA,GAAG,EAAC,cAaA;AAZH,IAAA,KAAK,EAAA,eAAA,CAAA,CAAE,KAAA,CAAA,YAAF,EACN;AAAA,iBAAA,KAAA;AAAA,gBAAA,WAAA;AAAA,eAAA,OAAA;AAAA,gBAAA,OAAA;AAAA,gBAAA,OAAA;AAAA,uBAAA;AAAA,KADM,CAAA;AAYF,GAfN,E,CAQE,mBAAA,CAME,KANF,EAME;AALA,IAAA,GAAG,EAAC,qDAKJ;AAJA,IAAA,EAAE,EAAC,aAIH;AAHA,IAAA,KAAqE,EAArE;AAAA,gBAAA,SAAA;AAAA,kBAAA,UAAA;AAAA,eAAA,KAAA;AAAA,aAAA,MAAA;AAAA,iBAAA;AAAA,KAGA;AAFC,IAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,KAAA,CAAA,YAAA,CAAa,OAAb,GAAoB,MAAtB;AAAA,KAAA,CAEN;AADA,IAAA,GAAG,EAAC;AACJ,GANF,C,CARF,E;;AAAA,GAgBI,CAxDN,CA2DI,EAFJ,WAEI,EADJ,mBAAA,CAAyF,QAAzF,EAAyF;AAAjF,IAAA,IAAI,EAAC,QAA4E;AAAlE,IAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAA6D;AAA7C,IAAA,KAAK,EAAC;AAAuC,GAAzF,EAAyE,SAAzE,CACI,CA7DN,C,qCA8DI,CAvEN,CAwEI,CApFN,CAwFI,EAHJ,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADJ,mBAAA,CAAyD,GAAzD,EAAyD;AAArD,IAAA,OAAK,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,aAAA,IAAA,QAAA,CAAA,aAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAAgD;AAA/B,IAAA,KAAK,EAAC;AAAyB,GAAzD,EAAiD,MAAjD,CACI,CAFN,CAGI,CA/FN,C","sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"profile\">\n      <div class=\"info\">\n        <h2>{{ username }} <span>님</span></h2>\n        <h3>{{ email }}</h3>\n      </div>\n    </div>\n    <div class=\"container\">\n      <ul class=\"sections\">\n        <li class=\"result\">\n          <p @click=\"section = 1\">가슴진단결과</p>\n        </li>\n        <li class=\"delivery\">\n          <p @click=\"section = 3\">배송 정보</p>\n        </li>\n        <li class=\"info\">\n          <p @click=\"section = 2\">회원 정보</p>\n        </li>\n      </ul>\n      <div class=\"showpart\">\n        <div v-if=\"section == 1\" class=\"result-list\">\n          <div class=\"result-box\">\n            <div>\n              <!-- <TestResult v-for=\"testItem in testItems\" :key=\"testItem._id\" :testItem=\"testItem\" /> -->\n              <TestResult />\n            </div>\n          </div>\n        </div>\n        <div v-if=\"section == 2\" class=\"information\">\n          <!-- <h2>기본 회원 정보</h2> -->\n          <div class=\"user-info\">\n            <table>\n              <!-- <label for=\"\">회원정보</label> -->\n              <tr>\n                <td>이름</td>\n                <td><input type=\"text\" v-model=\"username\" /></td>\n              </tr>\n              <tr>\n                <td>이메일</td>\n                <td>{{ email }}</td>\n              </tr>\n              <tr>\n                <td>휴대폰번호</td>\n                <td><input type=\"text\" @keyup=\"getPhoneMask(phone)\" v-model=\"phone\" /></td>\n              </tr>\n              <tr>\n                <td>생년월일</td>\n                <td><input type=\"date\" v-model=\"birthday\" /></td>\n              </tr>\n              <tr>\n                <td>혜택 알림 설정</td>\n                <td class=\"boxx\"><ToggleSwitch class=\"it\" v-model=\"agreePromotion\"></ToggleSwitch></td>\n              </tr>\n            </table>\n\n            <table>\n              <tr>\n                <td>우편번호</td>\n                <td><input @click=\"execDaumPostcode\" type=\"text\" v-model=\"postcode\" /></td>\n              </tr>\n              <tr>\n                <td>주소</td>\n                <td><input type=\"text\" v-model=\"address\" /></td>\n              </tr>\n              <tr>\n                <td>상세주소</td>\n                <td><input type=\"text\" v-model=\"extraAddress\" /></td>\n              </tr>\n            </table>\n\n            <div\n              class=\"postcode-search\"\n              ref=\"searchWindow\"\n              :style=\"searchWindow\"\n              style=\"z-index:100;border:1px solid;\n                    width:330px;height:400px;margin:5px 0;\n                    margin-bottom:16px;\"\n            >\n              <img\n                src=\"//t1.daumcdn.net/postcode/resource/images/close.png\"\n                id=\"btnFoldWrap\"\n                style=\"cursor:pointer;position:absolute;right:0px;top:-1px;z-index:1\"\n                @click=\"searchWindow.display = 'none'\"\n                alt=\"close\"\n              />\n            </div>\n          </div>\n          <div class=\"address-info\"></div>\n          <button type=\"button\" @click=\"submitChange\" class=\"btn-secondary btn-40\">회원정보 수정</button>\n        </div>\n      </div>\n    </div>\n    <div class=\"others\">\n      <a @click=\"deleteAccount\" class=\"delete-account\">탈퇴하기</a>\n    </div>\n  </div>\n</template>\n\n<script>\n\"use strict\";\n// import { fetchUserData } from '@/api/index';\nimport TestResult from \"@/components/TestResult.vue\";\nimport ToggleSwitch from \"@/components/ToggleSwitch.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"Mypage\",\n  data() {\n    return {\n      searchWindow: {\n        display: \"none\",\n        height: \"300px\",\n      },\n      email: \"\",\n      postcode: \"\",\n      address: \"\",\n      birthday: \"\",\n      extraAddress: \"\",\n      phone: \"\",\n      agreePromotion: false,\n      section: 1,\n      // testList: this.$store.state.user.test || [\"\",],\n      username: \"\",\n      testItems: [],\n    };\n  },\n  components: {\n    TestResult,\n    ToggleSwitch,\n  },\n  mounted() {\n    window.scrollTo(0, 0);\n    this.emitter.on(\"setCheckboxVal\", (state) => {\n      this.agreePromotion = state;\n    });\n  },\n  methods: {\n    execDaumPostcode() {\n      const currentScroll = Math.max(document.body.scrollTop, document.documentElement.scrollTop);\n      // eslint-disable-next-line\n      new daum.Postcode({\n        onComplete: (data) => {\n          if (data.userSelectedType === \"R\") {\n            this.address = data.roadAddress;\n          } else {\n            this.address = data.jibunAddress;\n          }\n          if (data.userSelectedType === \"R\") {\n            if (data.bname !== \"\" && /[동|로|가]$/g.test(data.bname)) {\n              this.extraAddress += data.bname;\n            }\n            if (data.buildingName !== \"\" && data.apartment === \"Y\") {\n              this.extraAddress += this.extraAddress !== \"\" ? `, ${data.buildingName}` : data.buildingName;\n            }\n            if (this.extraAddress !== \"\") {\n              this.extraAddress = ` (${this.extraAddress})`;\n            }\n          } else {\n            this.extraAddress = \"\";\n          }\n          this.postcode = data.zonecode;\n          this.$refs.extraAddress.focus();\n          this.searchWindow.display = \"none\";\n          document.body.scrollTop = currentScroll;\n        },\n        onResize: (size) => {\n          this.searchWindow.height = `${size.height}px`;\n        },\n        width: \"100%\",\n        height: \"100%\",\n      }).embed(this.$refs.searchWindow);\n      this.searchWindow.display = \"block\";\n    },\n    deleteAccount() {\n      this.emitter.emit(\"deleteAccountModal\", true);\n    },\n    getPhoneMask(val) {\n      let res = this.getMask(val);\n      this.phone = res;\n      //서버 전송 값에는 '-' 를 제외하고 숫자만 저장\n      // this.model.contact = this.contact.replace(/[^0-9]/g, '')\n    },\n\n    getMask(phoneNumber) {\n      if (!phoneNumber) return phoneNumber;\n      phoneNumber = phoneNumber.replace(/[^0-9]/g, \"\");\n\n      let res = \"\";\n      if (phoneNumber.length < 3) {\n        res = phoneNumber;\n      } else {\n        if (phoneNumber.substr(0, 2) == \"02\") {\n          if (phoneNumber.length <= 5) {\n            //02-123-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 3);\n          } else if (phoneNumber.length > 5 && phoneNumber.length <= 9) {\n            //02-123-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 3) + \"-\" + phoneNumber.substr(5);\n          } else if (phoneNumber.length > 9) {\n            //02-1234-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 4) + \"-\" + phoneNumber.substr(6);\n          }\n        } else {\n          if (phoneNumber.length < 8) {\n            res = phoneNumber;\n          } else if (phoneNumber.length == 8) {\n            res = phoneNumber.substr(0, 4) + \"-\" + phoneNumber.substr(4);\n          } else if (phoneNumber.length == 9) {\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 3) + \"-\" + phoneNumber.substr(6);\n          } else if (phoneNumber.length == 10) {\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 3) + \"-\" + phoneNumber.substr(6);\n          } else if (phoneNumber.length > 10) {\n            //010-1234-5678\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 4) + \"-\" + phoneNumber.substr(7);\n          }\n        }\n      }\n\n      return res;\n    },\n\n    async submitChange() {\n      const userData = {\n        usercode: this.$store.state.usercode,\n        email: this.email,\n        username: this.username,\n        phone: this.phone,\n        birthday: this.birthday,\n        agreePromotion: this.agreePromotion,\n      };\n\n      axios\n        .post(\"/users/edit/userInfo\", userData)\n        .then((result) => {\n          console.log(result.data);\n          if (result.data.success) {\n            console.log(result.data.message);\n            // 수정되었다는 알림 스낵바\n            let message = \"회원정보가 수정되었습니다.\";\n            this.emitter.emit(\"showToast\", message);\n          }\n        })\n        .catch((err) => console.log(err));\n\n      this.editKitData();\n    },\n\n    async editKitData() {\n      const kitData = {\n        usercode: this.username,\n        address: this.address,\n        extraAddress: this.extraAddress,\n        postcode: this.postcode,\n      };\n      axios.post(\"/kits/request/editInfo\", kitData).then((result) => {\n        if (result.data.success) {\n          console.log(result.data.message);\n        }\n      });\n    },\n\n    // 유저 정보 불러오는 API 호출\n    async fetchInfo() {\n      // const { data } = await fetchUserData();\n      const { data } = await axios.get(\"/users/getUserInfo\");\n      console.log(data);\n      // user data 받아오기\n      if (data.success) {\n        this.username = data.username;\n        this.email = data.email;\n        this.phone = data.phone;\n        this.birthday = data.birthday;\n        this.agreePromotion = data.agreePromotion;\n      }\n\n      //테스트 결과 목록 불러오기\n    },\n\n    async fetchKitInfo() {\n      const { data } = await axios.get(\"/kits/request/getKitInfo\");\n      let kitData = data.kitInfo;\n      console.log(data);\n      if (data.success) {\n        this.postcode = kitData.postcode;\n        this.address = kitData.address;\n        this.extraAddress = kitData.extraAddress;\n      } else {\n        console.log(data.message);\n      }\n    },\n  },\n  created() {\n    this.fetchInfo();\n    this.fetchKitInfo();\n    // this.fetchTestresults()\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n  height: 140vh;\n  background-color: $white;\n  display: flex;\n  flex-direction: column;\n  padding: 128px;\n  justify-content: flex-start;\n  align-items: center;\n\n  @media screen and (max-width: $md-breakpoint - 1px) {\n    padding: 100px 0;\n  }\n\n  .profile {\n    position: relative;\n    display: flex;\n    justify-content: flex-start;\n    padding: 32px;\n    margin: 8px;\n    height: 150px;\n    width: 700px;\n    background-color: $background;\n    border-radius: 8px;\n    box-shadow: 0px 4px 4px 0px $tertiary;\n\n    @media screen and (max-width: $md-breakpoint - 1px) {\n      width: 90%;\n    }\n\n    h2 {\n      @include text-style(24, $primary);\n      margin-bottom: 8px;\n    }\n    span {\n      @include text-style(18, $primary);\n    }\n    h3 {\n      @include text-style(18, $secondary);\n    }\n  }\n\n  .container {\n    height: 600px;\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: flex-start;\n    padding: 16px;\n    margin: 8px;\n    width: 700px;\n    background-color: $background;\n    border-radius: 8px;\n    box-shadow: 0px 4px 4px 0px $tertiary;\n\n    @media screen and (max-width: $md-breakpoint - 1px) {\n      width: 90%;\n      height: 480px;\n    }\n\n    .sections {\n      @include text-style(18, $secondary);\n      display: flex;\n      justify-content: space-around;\n      padding: 16px;\n      border-bottom: 1px solid $tertiary;\n\n      @media screen and (max-width: $md-breakpoint - 1px) {\n        @include text-style(16, $secondary);\n      }\n\n      li {\n        &:hover {\n          opacity: 0.7;\n        }\n      }\n    }\n\n    .result-box {\n      width: 650px;\n      height: 430px;\n      overflow-y: scroll;\n\n      @media screen and (max-width: $md-breakpoint - 1px) {\n        height: 390px;\n      }\n    }\n\n    .result-box::-webkit-scrollbar {\n      background-color: rgba(0, 0, 0, 0);\n    }\n\n    .showpart {\n      display: flex;\n      justify-content: center;\n      align-items: flex-start;\n      margin: 8px;\n\n      .information {\n        width: 95%;\n        display: flex;\n        height: 500px;\n        flex-direction: column;\n        align-items: center;\n\n        @media screen and (max-width: $md-breakpoint - 1) {\n          height: 300px;\n        }\n\n        button {\n          width: 150px;\n          position: absolute;\n          bottom: 5%;\n        }\n\n        table {\n          @include text-style(18, $primary);\n          margin: 16px;\n          margin-left: 150px;\n          width: 600px;\n          border-spacing: 0 16px;\n          @media screen and (max-width: $md-breakpoint - 1px) {\n            @include text-style(14, $primary);\n            width: 270px;\n            margin-left: 30px;\n            border-spacing: 0 8px;\n          }\n\n          tr {\n            margin-bottom: 8px;\n          }\n        }\n      }\n\n      .result-list {\n        display: flex;\n        width: 100%;\n      }\n    }\n  }\n\n  .others {\n    display: flex;\n    .delete-account {\n      @include text-style(13, $tertiary);\n      text-decoration: underline;\n    }\n  }\n\n  .postcode-search {\n    position: absolute;\n    left: 30%;\n    top: 65%;\n    @media screen and (max-width: $md-breakpoint - 1) {\n      /* margin-left: ; */\n      position: absolute;\n      left: 0%;\n    }\n  }\n\n  input {\n    width: 300px;\n\n    @media screen and (max-width: $md-breakpoint - 1) {\n      width: 150px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}