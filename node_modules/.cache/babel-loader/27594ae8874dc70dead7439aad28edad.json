;AA+CN,QAAA,OAAO,EAAE,EA/CH;AAgDN,QAAA,OAAO,EAAE,EAhDH;AAkDN;AACA,QAAA,UAAU,EAAE,EAnDN;AAoDN,QAAA,SAAS,EAAE,EApDL;AAqDN,QAAA,QAAQ,EAAE,EArDJ;AAuDN;AACA,QAAA,QAAQ,EAAE,EAxDJ;AAyDN,QAAA,GAAG,EAAE,EAzDC;AA0DN,QAAA,SAAS,EAAE,EA1DL;AA2DN,QAAA,KAAK,EAAE,EA3DD;AA6DN;AACA,QAAA,OAAO,EAAE,EA9DH;AA+DN,QAAA,UAAU,EAAE;AA/DN;AADH,KAAP;AAmED,GArEY;AAsEb,EAAA,OAtEa,qBAsEH;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;AACA,QAAM,GAAE,GAAI,QAAQ,CAAC,cAAT,CAAwB,SAAxB,CAAZ;AACA,QAAM,OAAM,GAAI,IAAI,KAAJ,CAAU,GAAV,EAAe;AAC7B,MAAA,IAAI,EAAE,OADuB;AAE7B,MAAA,IAAI,EAAE;AACJ,QAAA,MAAM,EAAE,CAAC,IAAD,EAAO,QAAP,EAAiB,UAAjB,CADJ;AAEJ,QAAA,QAAQ,EAAE,CACR;AACE,UAAA,KAAK,YAAK,KAAK,MAAL,CAAY,QAAjB,YADP;AAEE,UAAA,IAAI,EAAE,CAAC,CAAD,EAAI,CAAJ,EAAO,CAAP,CAFR;AAGE,UAAA,eAAe,EAAE,CACf,yBADe,EAEf,yBAFe,EAGf,yBAHe,CAHnB;AAQE,UAAA,WAAW,EAAE,CACX,uBADW,EAEX,uBAFW,EAGX,uBAHW,CARf;AAaE,UAAA,WAAW,EAAE;AAbf,SADQ;AAFN,OAFuB;AAsB7B,MAAA,OAAO,EAAE;AACP,QAAA,MAAM,EAAE;AACN,UAAA,MAAM,EAAE;AACN,YAAA,QAAQ,EAAE;AADJ;AADF,SADD;AAMP,QAAA,KAAK,EAAE;AACL,UAAA,UAAU,EAAE;AACV,YAAA,OAAO,EAAE;AADC,WADP;AAIL,UAAA,KAAK,EAAE;AACL,YAAA,YAAY,EAAE,CADT;AAEL,YAAA,YAAY,EAAE;AAFT;AAJF;AANA;AAtBoB,KAAf,CAAhB;AAuCA,IAAA,OAAO,CAAC,GAAR,CAAY,OAAZ;AACD,GAjHY;AAkHb,EAAA,OAAO,EAAE;AACD,IAAA,eADC,6BACiB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAChB,KAAK,CAAC,GAAN,CAAU,uBAAV,EACH,IADG,CACE,UAAC,MAAD,EAAY;AAChB;AACA,sBAAI,MAAM,CAAC,IAAP,CAAY,OAAhB,EAAyB;AACvB,wBAAI,IAAG,GAAI,MAAM,CAAC,IAAP,CAAY,MAAZ,CAAmB,CAAnB,CAAX;AACA,oBAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;AACA,oBAAA,KAAI,CAAC,MAAL,GAAc,IAAd;AACA,wBAAI,IAAI,CAAC,YAAL,KAAsB,KAA1B,EAAiC,KAAI,CAAC,MAAL,CAAY,WAAZ,GAA0B,OAAO,CAAC,wBAAD,CAAjC,CAAjC,KACK,IAAI,IAAI,CAAC,YAAL,KAAsB,KAA1B,EAAiC,KAAI,CAAC,MAAL,CAAY,WAAZ,GAA0B,OAAO,CAAC,wBAAD,CAAjC,CAAjC,KACA,IAAI,IAAI,CAAC,YAAL,KAAsB,KAA1B,EAAiC,KAAI,CAAC,MAAL,CAAY,WAAZ,GAA0B,OAAO,CAAC,wBAAD,CAAjC;AACtC,wBAAI,IAAI,CAAC,aAAL,KAAuB,KAA3B,EAAkC,KAAI,CAAC,MAAL,CAAY,YAAZ,GAA2B,OAAO,CAAC,yBAAD,CAAlC,CAAlC,KACK,IAAI,IAAI,CAAC,aAAL,KAAuB,KAA3B,EAAkC,KAAI,CAAC,MAAL,CAAY,YAAZ,GAA2B,OAAO,CAAC,yBAAD,CAAlC,CAAlC,KACA,IAAI,IAAI,CAAC,aAAL,KAAuB,KAA3B,EAAkC,KAAI,CAAC,MAAL,CAAY,YAAZ,GAA2B,OAAO,CAAC,yBAAD,CAAlC;AACvC,wBAAI,IAAI,CAAC,cAAL,KAAwB,KAA5B,EAAmC,KAAI,CAAC,MAAL,CAAY,aAAZ,GAA4B,OAAO,CAAC,0BAAD,CAAnC,CAAnC,KACK,IAAI,IAAI,CAAC,cAAL,KAAwB,KAA5B,EAAmC,KAAI,CAAC,MAAL,CAAY,aAAZ,GAA4B,OAAO,CAAC,0BAAD,CAAnC,CAAnC,KACA,IAAI,IAAI,CAAC,cAAL,KAAwB,KAA5B,EAAmC,KAAI,CAAC,MAAL,CAAY,aAAZ,GAA4B,OAAO,CAAC,0BAAD,CAAnC;AAC1C;AACD,iBAjBG,EAkBH,KAlBG,CAkBG,OAAO,CAAC,GAlBX,CADgB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBvB;AArBM,GAlHI;AAyIb,EAAA,OAzIa,qBAyIH;AACR,SAAK,eAAL;AACD;AA3IY,CAAf","sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"form\">\n      <div class=\"main-title\">\n        <span>{{ result.date }}</span>\n        <h1>{{ result.username }}님의 가슴 진단 결과</h1>\n      </div>\n      <!-- breast result -->\n      <div class=\"breast-result\">\n        <h2 class=\"result-title\">내 가슴 결과</h2>\n        <!-- container : size -->\n        <div class=\"container size\">\n          <div class=\"container-title\">\n            <h3>사이즈&nbsp;<i class=\"fas fa-check\"></i></h3>\n          </div>\n          <div class=\"container-content\">\n            <p>\n              브라 사이즈 공식에 따라,\n              <span>{{ result.size }}</span> 사이즈가 도출되었습니다. <br />\n              디자인에 따라 사이즈가 조금씩 다르므로 절대적인 정답은 아니랍니다!\n            </p>\n          </div>\n        </div>\n        <!-- container : breast underline -->\n        <div class=\"container breast-underline\">\n          <div class=\"container-title\">\n            <h3>가슴 밑선</h3>\n          </div>\n          <div class=\"container-content\">\n            <div class=\"image-box\">\n              <div class=\"image-item left\">\n                <label for=\"leftImg\">왼쪽&nbsp;<i class=\"fas fa-sort-down\"></i></label>\n                <img v-if=\"!result.leftLowerShapeImg\" src=\"@/assets/noImage.png\" alt=\"\" />\n                <img v-if=\"result.leftLowerShapeImg\" src=\"/breastTest/getImg/leftLowerShapeImg\" id=\"leftImg\" alt=\"\" />\n              </div>\n              <div class=\"image-item right\">\n                <label for=\"rightImg\">오른쪽&nbsp;<i class=\"fas fa-sort-down\"></i></label>\n                <img v-if=\"!result.rightLowerShapeImg\" src=\"@/assets/noImage.png\" alt=\"\" />\n                <img v-if=\"result.rightLowerShapeImg\" src=\"/breastTest/getImg/rightLowerShapeImg\" id=\"rightImg\" alt=\"\" />\n              </div>\n              <!-- 응답 X 사진은 응답 미완료 표시 (이미지 칸 중간에 문구 띄우기) -->\n            </div>\n            <div class=\"underline-table info-table\">\n\n              <!-- column 3 -->\n              <div class=\"left-values\">\n                <div class=\"value\">{{ result.leftWidth }}</div>\n                <div class=\"value\">{{ result.leftInnerCurve }}</div>\n                <div class=\"value\">{{ result.leftOuterCurve }}</div>\n                <div class=\"value long\">{{ result.leftFinish }}</div>\n              </div>\n              <!-- column 1 -->\n              <div class=\"table-head\">\n                <div class=\"head\">너비</div>\n                <div class=\"head\">안쪽 곡률</div>\n                <div class=\"head\">바깥쪽 곡률</div>\n                <div class=\"head\">끝부분 위치</div>\n              </div>              \n              <!-- column 2 -->\n              <div class=\"right-values\">\n                <div class=\"value\">{{ result.rightWidth }}</div>\n                <div class=\"value\">{{ result.rightInnerCurve }}</div>\n                <div class=\"value\">{{ result.rightOuterCurve }}</div>\n                <div class=\"value long\">{{ result.rightFinish }}</div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"container fat-range\">\n          <div class=\"container-title\">\n            <h3>지방 분포</h3>\n          </div>\n          <div class=\"container-content\">\n            <div class=\"area-dist-type\">\n              <img v-if=\"result.distAreaName == '돌출형'\" src=\"@/assets/distArea/dolchool.png\" alt=\"\" />\n              <img v-if=\"result.distAreaName == '고른형'\" src=\"@/assets/distArea/even_1.png\" alt=\"\" />\n              <img v-if=\"result.distAreaName == '완만형'\" src=\"@/assets/distArea/smooth.png\" alt=\"\" />\n              <div class=\"detail-info\">\n                <h4>면적 분포 유형</h4>\n                <p>\n                  <i class=\"fas fa-arrow-right\"></i>&nbsp;&nbsp;\n                  <span>{{ result.distAreaName }}</span>\n                </p>\n                <p class=\"details\">{{ result.distAreaInfo }}</p>\n              </div>\n            </div>\n            <div class=\"inout-dist-type\">\n              <img v-if=\"result.distInOutName == '안쪽형'\" src=\"@/assets/distArea/inside.png\" alt=\"\" />\n              <img v-if=\"result.distInOutName == '고른형'\" src=\"@/assets/distArea/even_2.png\" alt=\"\" />\n              <img v-if=\"result.distInOutName == '바깥형'\" src=\"@/assets/distArea/outside.png\" alt=\"\" />\n              <div class=\"detail-info\">\n                <h4>안밖 분포 유형</h4>\n                <p>\n                  <i class=\"fas fa-arrow-right\"></i>&nbsp;&nbsp;\n                  <span>{{ result.distInOutName }}</span>\n                </p>\n                <p class=\"details\">{{ result.distInOutInfo }}</p>\n              </div>\n            </div>\n            <div class=\"updown-dist-type\">\n              <img v-if=\"result.distUpDownName == '위쪽형'\" src=\"@/assets/distArea/upside.png\" alt=\"\" />\n              <img v-if=\"result.distUpDownName == '고른형'\" src=\"@/assets/distArea/even_3.png\" alt=\"\" />\n              <img v-if=\"result.distUpDownName == '아래형'\" src=\"@/assets/distArea/downside.png\" alt=\"\" />\n              <div class=\"detail-info\">\n                <h4>위아래 분포 유형</h4>\n                <p>\n                  <i class=\"fas fa-arrow-right\"></i>&nbsp;&nbsp;\n                  <span>{{ result.distUpDownName }}</span>\n                </p>\n                <p class=\"details\">{{ result.distUpDownInfo }}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"container position\">\n          <div class=\"container-title\">\n            <h3>위치 정보</h3>\n          </div>\n          <div class=\"container-content\">\n            <!-- graph -->\n            <div class=\"graph-box\">\n              <canvas id=\"myChart\" width=\"300\" height=\"300\"></canvas>\n            </div>\n            <div class=\"partition\"></div>\n            <div class=\"graph-details\">\n              <div class=\"detail-item\">\n                <div class=\"detail-head\">\n                  <h4><span>1</span>처짐</h4>\n                </div>\n                <div class=\"detail-text\">\n                  <i class=\"fas fa-arrow-right\"></i>\n                  <span>{{ result.sagInfo }}</span>\n                </div>\n              </div>\n              <div class=\"detail-item\">\n                <div class=\"detail-head\">\n                  <h4><span>2</span>유두 벌어짐</h4>\n                  <p>( 유두가 바깥을 향하는 정도 )</p>\n                </div>\n                <div class=\"detail-text\">\n                  <i class=\"fas fa-arrow-right\"></i>\n                  <span>{{ result.dirInfo }}</span>\n                </div>\n              </div>\n              <div class=\"detail-item\">\n                <div class=\"detail-head\">\n                  <h4><span>3</span>가슴 사이 거리</h4>\n                  <p>( 양쪽 가슴이 가까이 붙어져 있는 정도 )</p>\n                </div>\n                <div class=\"detail-text\">\n                  <i class=\"fas fa-arrow-right\"></i>\n                  <span>{{ result.gapInfo }}</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"container diff-breast\">\n          <div class=\"container-title\">\n            <h3>짝가슴 여부</h3>\n          </div>\n          <div class=\"container-content\">\n            <div class=\"diff-table info-table\">\n              <!-- column 1 -->\n              <div class=\"table-head\">\n                <div class=\"head\">양쪽 부피 차이</div>\n                <div class=\"head\">양쪽 너비 차이</div>\n              </div>\n              <!-- column 2 -->\n              <div class=\"right-values\">\n                <div class=\"value\">{{ result.volumeDiff }}</div>\n                <div class=\"value\">{{ result.widthDiff }}</div>\n              </div>\n            </div>\n            <div class=\"detail-text\">\n              <i class=\"fas fa-arrow-right\"></i>\n              <span>{{ result.diffInfo }}</span>\n            </div>\n          </div>\n        </div>\n        <div class=\"container result-else\">\n          <div class=\"container-title\">\n            <h3>가슴 외 결과</h3>\n          </div>\n          <div class=\"else-table info-table\">\n            <!-- column 1 -->\n            <div class=\"table-head\">\n              <div class=\"head\">어깨</div>\n              <div class=\"head\">흉곽</div>\n              <div class=\"head\">부유방</div>\n              <div class=\"head\">살결</div>\n            </div>\n            <!-- column 2 -->\n            <div class=\"right-values\">\n              <div class=\"value\">{{ result.shoulder }}</div>\n              <div class=\"value\">{{ result.rib }}</div>\n              <div class=\"value\">{{ result.accBreast }}</div>\n              <div class=\"value\">{{ result.flesh }}</div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- bra review -->\n      <div class=\"bra-review\">\n        <h2 class=\"result-title\">브라 착용 리뷰</h2>\n        <div class=\"container wearing\">\n          <div class=\"container-title\">\n            <h3>올바르게 입고 있었을까?</h3>\n          </div>\n          <div class=\"container-content\">\n            <p>{{ result.wearing }}</p>\n          </div>\n        </div>\n        <div class=\"container hard-to-wear\">\n          <div class=\"container-title\">\n            <h3>이제까지 힘드셨죠?</h3>\n          </div>\n          <div class=\"container-content\">\n            <p>{{ result.hardToWear }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"comments\">\n      <p>내 가슴 결과와 브라 착용 실태를 고려한</p>\n    </div>\n    <div class=\"button-container\">\n      <div class=\"buttons\">\n        <button type=\"button\" class=\"btn-primary btn-55\">추천브라 보러가기</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Chart from \"chart.js\";\n\nexport default {\n  data() {\n    return {\n      result: {\n        date: \"\",\n        username: \"\",\n\n        size: \"\",\n\n        leftLowerShapeImg: null,\n        rightLowerShapeImg: null,\n\n        //왼\n        leftWidth: \"\",\n        leftInnerCurve: \"\",\n        leftOuterCurve: \"\",\n        leftFinish: \"\",\n\n        //오\n        rightWidth: \"\",\n        rightInnerCurve: \"\",\n        rightOuterCurve: \"\",\n        rightFinish: \"\",\n\n        //유형에따른이미지경로\n        distAreaImg: null,\n        distInOutImg: null,\n        distUpDownImg: null,\n\n        //유형명\n        distAreaName: \"\",\n        distInOutName: \"\",\n        distUpDownName: \"\",\n\n        //유형에따른설명\n        distAreaInfo: \"\",\n        distInOutInfo: \"\",\n        distUpDownInfo: \"\",\n\n        //그래프쪽은 아직 보류\n        sag: \"\", //처짐정도, 0,1,2 셋중 하나, 0-하 1-중 2-상, 그래프에 이용\n        dir: \"\", //벌어짐정도 0,1,2 셋중 하나, 0-하 1-중 2-상, 그래프에 이용\n        gap: \"\", //가슴사이거리 0,1,2 셋중 하나, 0-하 1-중 2-상, 그래프에 이용\n\n        //변수 따로 안쓰고 위에꺼 0,1,2에 따라 다르게 해도 되긴 될듯\n        // sagDegree: \"상\", //처짐정도, 상중하\n        // dirDegree: \"상\", //벌어짐정도, 상중하\n        // gapDegree: \"상\", //가슴사이거리, 상중하\n\n        sagInfo: \"\",\n        dirInfo: \"\",\n        gapInfo: \"\",\n\n        //짝가슴 여부\n        volumeDiff: \"\",\n        widthDiff: \"\",\n        diffInfo: \"\",\n\n        //가슴 외 결과\n        shoulder: \"\",\n        rib: \"\",\n        accBreast: \"\",\n        flesh: \"\",\n\n        //올바르게 입고 있었을까?\n        wearing: \"\",\n        hardToWear: \"\",\n      },\n    };\n  },\n  mounted() {\n    window.scrollTo(0, 0);\n    const ctx = document.getElementById(\"myChart\");\n    const myChart = new Chart(ctx, {\n      type: \"radar\",\n      data: {\n        labels: [\"처짐\", \"유두 벌어짐\", \"가슴 사이 거리\"],\n        datasets: [\n          {\n            label: `${this.result.username} 님`,\n            data: [1, 2, 1],\n            backgroundColor: [\n              \"rgba(255, 99, 132, 0.2)\",\n              \"rgba(54, 162, 235, 0.2)\",\n              \"rgba(255, 206, 86, 0.2)\",\n            ],\n            borderColor: [\n              \"rgba(255, 99, 132, 1)\",\n              \"rgba(54, 162, 235, 1)\",\n              \"rgba(255, 206, 86, 1)\",\n            ],\n            borderWidth: 1,\n          },\n        ],\n      },\n      options: {\n        legend: {\n          labels: {\n            fontSize: 12,\n          },\n        },\n        scale: {\n          angleLines: {\n            display: false,\n          },\n          ticks: {\n            suggestedMin: 0,\n            suggestedMax: 2,\n          },\n        },\n      },\n    });\n    console.log(myChart);\n  },\n  methods: {\n    async fetchResultData() {\n      await axios.get(\"/breastTest/getResult\")\n        .then((result) => {\n          // console.log(result.data);\n          if (result.data.success) {\n            let data = result.data.result[0];\n            console.log(data);\n            this.result = data;\n            if (data.distAreaName === \"돌출형\") this.result.distAreaImg = require(\"@/assets/dist_area.png\");\n            else if (data.distAreaName === \"고른형\") this.result.distAreaImg = require(\"@/assets/dist_area.png\");\n            else if (data.distAreaName === \"완만형\") this.result.distAreaImg = require(\"@/assets/dist_area.png\");\n            if (data.distInOutName === \"위쪽형\") this.result.distInOutImg = require(\"@/assets/dist_inout.png\");\n            else if (data.distInOutName === \"고른형\") this.result.distInOutImg = require(\"@/assets/dist_inout.png\");\n            else if (data.distInOutName === \"아래형\") this.result.distInOutImg = require(\"@/assets/dist_inout.png\");\n            if (data.distUpDownName === \"안쪽형\") this.result.distUpDownImg = require(\"@/assets/dist_updown.png\");\n            else if (data.distUpDownName === \"고른형\") this.result.distUpDownImg = require(\"@/assets/dist_updown.png\");\n            else if (data.distUpDownName === \"바깥형\") this.result.distUpDownImg = require(\"@/assets/dist_updown.png\");\n          }\n        })\n        .catch(console.log);\n    },\n  },\n  created() {\n    this.fetchResultData();\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n*:not(i):not(button) {\n  font-family: $font-main, sans-serif !important;\n}\n.body {\n  /* height: 570vh; */\n  height: auto;\n  background-color: $gray;\n  display: flex-start;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  padding: 60px 180px;\n\n  @media screen and (max-width: $md-breakpoint) {\n    padding: 40px 8px;\n    /* height: 4500px; */\n    height: auto;\n  }\n\n  .form {\n    padding: 24px;\n    display: flex;\n    justify-content: center;\n    box-shadow: 1px 4px 4px 1px $border;\n    flex-direction: column;\n    background-color: $white;\n    width: 100%;\n    /* height: 900px; */\n    margin-top: 96px;\n    @media screen and (max-width: $md-breakpoint - 1px) {\n      padding: 16px;\n    }\n\n    .main-title {\n      display: flex;\n      justify-content: center;\n      flex-direction: column;\n\n      span {\n        @include text-style(16, $secondary);\n        text-align: center;\n        /* margin: 8px; */\n      }\n\n      h1 {\n        @include text-style(28, $primary);\n        text-align: center;\n        margin: 8px;\n      }\n    }\n\n    .breast-result {\n      h2 {\n        text-align: center;\n      }\n    }\n\n    .result-title {\n      @include text-style