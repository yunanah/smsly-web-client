R,CAAY,MAAM,CAAC,IAAnB;;AACA,sBAAI,MAAM,CAAC,IAAP,CAAY,OAAhB,EAAyB;AACvB,oBAAA,OAAO,CAAC,GAAR,CAAY,MAAM,CAAC,IAAP,CAAY,OAAxB,EADuB,CAEvB;;AACA,wBAAI,OAAM,GAAI,gBAAd;;AACA,oBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,WAAlB,EAA+B,OAA/B;AACF;AACD,iBAVH,EAWG,KAXH,CAWS,UAAC,GAAD;AAAA,yBAAS,OAAO,CAAC,GAAR,CAAY,GAAZ,CAAT;AAAA,iBAXT;;AAaA,gBAAA,MAAI,CAAC,WAAL;;AAvBmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBpB,KAtHM;AAwHD,IAAA,WAxHC,yBAwHa;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACZ,gBAAA,OADY,GACF;AACd,kBAAA,KAAK,EAAE,MAAI,CAAC,QADE;AAEd,kBAAA,OAAO,EAAE,MAAI,CAAC,OAFA;AAGd,kBAAA,YAAY,EAAE,MAAI,CAAC,YAHL;AAId,kBAAA,QAAQ,EAAE,MAAI,CAAC;AAJD,iBADE;AAOlB,gBAAA,KAAK,CAAC,IAAN,CAAW,wBAAX,EAAqC,OAArC,EAA8C,IAA9C,CAAmD,UAAC,MAAD,EAAY;AAC7D,sBAAI,MAAM,CAAC,IAAP,CAAY,OAAhB,EAAyB;AACvB,oBAAA,OAAO,CAAC,GAAR,CAAY,MAAM,CAAC,IAAP,CAAY,OAAxB;AACF;AACD,iBAJD;;AAPkB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYnB,KApIM;AAsIP;AACM,IAAA,SAvIC,uBAuIW;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEO,KAAK,CAAC,GAAN,CAAU,oBAAV,CAFP;;AAAA;AAAA;AAER,gBAAA,IAFQ,oBAER,IAFQ;AAGhB,gBAAA,OAAO,CAAC,GAAR,CAAY,IAAZ,EAHgB,CAIhB;;AACA,oBAAI,IAAI,CAAC,OAAT,EAAkB;AAChB,kBAAA,MAAI,CAAC,QAAL,GAAgB,IAAI,CAAC,QAArB;AACA,kBAAA,MAAI,CAAC,KAAL,GAAa,IAAI,CAAC,KAAlB;AACA,kBAAA,MAAI,CAAC,KAAL,GAAa,IAAI,CAAC,KAAlB;AACA,kBAAA,MAAI,CAAC,QAAL,GAAgB,IAAI,CAAC,QAArB;AACA,kBAAA,MAAI,CAAC,cAAL,GAAsB,IAAI,CAAC,cAA3B;AACF,iBAXgB,CAahB;;;AAbgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcjB,KArJM;AAuJD,IAAA,YAvJC,0BAuJc;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACI,KAAK,CAAC,GAAN,CAAU,0BAAV,CADJ;;AAAA;AAAA;AACX,gBAAA,IADW,qBACX,IADW;AAEf,gBAAA,OAFe,GAEL,IAAI,CAAC,OAFA;AAGnB,gBAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;;AACA,oBAAI,IAAI,CAAC,OAAT,EAAkB;AAChB,kBAAA,MAAI,CAAC,QAAL,GAAgB,OAAO,CAAC,QAAxB;AACA,kBAAA,MAAI,CAAC,OAAL,GAAe,OAAO,CAAC,OAAvB;AACA,kBAAA,MAAI,CAAC,YAAL,GAAoB,OAAO,CAAC,YAA5B;AACF,iBAJA,MAIO;AACL,kBAAA,OAAO,CAAC,GAAR,CAAY,IAAI,CAAC,OAAjB;AACF;;AAVmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWpB;AAlKM,GAzBI;AA6Lb,EAAA,OA7La,qBA6LH;AACR,SAAK,SAAL;AACA,SAAK,YAAL,GAFQ,CAGR;AACD;AAjMY,CAAf","sourcesContent":["<template>\n    <transition name=\"modal\" appear>\n    <form class=\"modal\">\n      <div class=\"modal__window\">\n        <span class=\"close_button\" @click=\"close\">\n          <i class=\"fas fa-times\"></i>\n        </span>\n        <div class=\"modal-title\">\n          <h2>회원 정보 수정</h2>\n        </div>\n        <div class=\"selection\">\n          <div @click=\"step = 1\" class=\"option\">\n            <h3>회원 정보</h3>\n          </div>\n          <div @click=\"step = 2\" class=\"option\">\n            <h3>배송 정보</h3>\n          </div>\n        </div>\n        <div class=\"showpart\">\n          <div v-if=\"step == 1\" class=\"user-info\">\n            <table>\n              <!-- <label for=\"\">회원정보</label> -->\n              <tr>\n                <td>이름</td>\n                <td><input type=\"text\" v-model=\"username\"></td>\n              </tr>\n              <tr>\n                <td>이메일</td>\n                <td>{{ email }}</td>\n              </tr>\n              <tr>\n                <td>휴대폰번호</td>\n                <td><input type=\"text\" @keyup=\"getPhoneMask(phone)\" v-model=\"phone\"></td>\n              </tr>\n              <tr>\n                <td>생년월일</td>\n                <td><input type=\"date\" v-model=\"birthday\"></td>\n              </tr>\n              <tr>\n                <td>혜택 알림 설정</td>\n                <td class=\"boxx\"><ToggleSwitch class=\"it\" v-model=\"agreePromotion\"></ToggleSwitch></td>\n              </tr>             \n            </table>\n\n          </div>\n          <div v-if=\"step == 2\" class=\"delivery-info\">\n            <table>\n              <tr>\n                <td>우편번호</td>\n                <td><input @click=\"execDaumPostcode\" type=\"text\" v-model=\"postcode\"></td>\n              </tr>\n              <tr>\n                <td>주소</td>\n                <td><input type=\"text\" v-model=\"address\"></td>\n              </tr>\n              <tr>\n                <td>상세주소</td>\n                <td><input type=\"text\" v-model=\"extraAddress\"></td>\n              </tr> \n            </table>\n            <div\n              class=\"postcode-search\"\n              ref=\"searchWindow\"\n              :style=\"searchWindow\"\n              style=\"z-index:100;border:1px solid;\n                    width:236px;height:224px;margin:5px 0;\n                    margin-bottom:16px;position:relative;\"\n            >\n              <img\n                src=\"//t1.daumcdn.net/postcode/resource/images/close.png\"\n                id=\"btnFoldWrap\"\n                style=\"cursor:pointer;position:absolute;right:0px;top:-1px;z-index:1\"\n                @click=\"searchWindow.display = 'none'\"\n                alt=\"close\"\n              >\n            </div>\n          </div>\n\n          <!-- <button class=\"btn-primary btn-48\">보기</button> -->\n        </div>\n      </div>\n      <div class=\"modal__overlay\" @click=\"close\"></div>\n    </form>\n  </transition>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'MemberInfoRevisionModal',\n  data() {\n    return {\n      step: 1,\n      searchWindow: {\n        display: \"none\",\n        height: \"200px\",\n      },\n      email: \"\",\n      postcode: \"\",\n      address: \"\",\n      birthday: \"\",\n      extraAddress: \"\",\n      phone: \"\",\n      agreePromotion: false,\n      section: 1,\n      // testList: this.$store.state.user.test || [\"\",],\n      username: \"\",\n      testItems: [],      \n    }\n  },\n  computed: {\n\n  },\n  methods: {\n    close() {\n      this.emitter.emit('MemberInfoRevisionModal', false)\n    },\n    execDaumPostcode() {\n      const currentScroll = Math.max(document.body.scrollTop, document.documentElement.scrollTop);\n      // eslint-disable-next-line\n      new daum.Postcode({\n        onComplete: (data) => {\n          if (data.userSelectedType === \"R\") {\n            this.address = data.roadAddress;\n          } else {\n            this.address = data.jibunAddress;\n          }\n          if (data.userSelectedType === \"R\") {\n            if (data.bname !== \"\" && /[동|로|가]$/g.test(data.bname)) {\n              this.extraAddress += data.bname;\n            }\n            if (data.buildingName !== \"\" && data.apartment === \"Y\") {\n              this.extraAddress += this.extraAddress !== \"\" ? `, ${data.buildingName}` : data.buildingName;\n            }\n            if (this.extraAddress !== \"\") {\n              this.extraAddress = ` (${this.extraAddress})`;\n            }\n          } else {\n            this.extraAddress = \"\";\n          }\n          this.postcode = data.zonecode;\n          this.$refs.extraAddress.focus();\n          this.searchWindow.display = \"none\";\n          document.body.scrollTop = currentScroll;\n        },\n        onResize: (size) => {\n          this.searchWindow.height = `${size.height}px`;\n        },\n        width: \"100%\",\n        height: \"100%\",\n      }).embed(this.$refs.searchWindow);\n      this.searchWindow.display = \"block\";\n    },\n    deleteAccount() {\n      this.emitter.emit(\"deleteAccountModal\", true);\n    },\n    openDeliveryModal() {\n      this.emitter.emit('DeliveryModal', true);\n    },\n    openRevisionModal() {\n      this.emitter.emit('MemberInfoRevisionModal', true);\n    },\n    getPhoneMask(val) {\n      let res = this.getMask(val);\n      this.phone = res;\n      //서버 전송 값에는 '-' 를 제외하고 숫자만 저장\n      // this.model.contact = this.contact.replace(/[^0-9]/g, '')\n    },\n\n    getMask(phoneNumber) {\n      if (!phoneNumber) return phoneNumber;\n      phoneNumber = phoneNumber.replace(/[^0-9]/g, \"\");\n\n      let res = \"\";\n      if (phoneNumber.length < 3) {\n        res = phoneNumber;\n      } else {\n        if (phoneNumber.substr(0, 2) == \"02\") {\n          if (phoneNumber.length <= 5) {\n            //02-123-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 3);\n          } else if (phoneNumber.length > 5 && phoneNumber.length <= 9) {\n            //02-123-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 3) + \"-\" + phoneNumber.substr(5);\n          } else if (phoneNumber.length > 9) {\n            //02-1234-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 4) + \"-\" + phoneNumber.substr(6);\n          }\n        } else {\n          if (phoneNumber.length < 8) {\n            res = phoneNumber;\n          } else if (phoneNumber.length == 8) {\n            res = phoneNumber.substr(0, 4) + \"-\" + phoneNumber.substr(4);\n          } else if (phoneNumber.length == 9) {\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 3) + \"-\" + phoneNumber.substr(6);\n          } else if (phoneNumber.length == 10) {\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 3) + \"-\" + phoneNumber.substr(6);\n          } else if (phoneNumber.length > 10) {\n            //010-1234-5678\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 4) + \"-\" + phoneNumber.substr(7);\n          }\n        }\n      }\n\n      return res;\n    },\n\n    async submitChange() {\n      const userData = {\n        PK_ID: this.$store.state.PK_ID,\n        email: this.email,\n        username: this.username,\n        phone: this.phone,\n        birthday: this.birthday,\n        agreePromotion: this.agreePromotion,\n      };\n\n      axios\n        .post(\"/users/edit/userInfo\", userData)\n        .then((result) => {\n          console.log(result.data);\n          if (result.data.success) {\n            console.log(result.data.message);\n            // 수정되었다는 알림 스낵바\n            let message = \"회원정보가 수정되었습니다.\";\n            this.emitter.emit(\"showToast\", message);\n          }\n        })\n        .catch((err) => console.log(err));\n\n      this.editKitData();\n    },\n\n    async editKitData() {\n      const kitData = {\n        PK_ID: this.username,\n        address: this.address,\n        extraAddress: this.extraAddress,\n        postcode: this.postcode,\n      };\n      axios.post(\"/kits/request/editInfo\", kitData).then((result) => {\n        if (result.data.success) {\n          console.log(result.data.message);\n        }\n      });\n    },\n\n    // 유저 정보 불러오는 API 호출\n    async fetchInfo() {\n      // const { data } = await fetchUserData();\n      const { data } = await axios.get(\"/users/getUserInfo\");\n      console.log(data);\n      // user data 받아오기\n      if (data.success) {\n        this.username = data.username;\n        this.email = data.email;\n        this.phone = data.phone;\n        this.birthday = data.birthday;\n        this.agreePromotion = data.agreePromotion;\n      }\n\n      //테스트 결과 목록 불러오기\n    },\n\n    async fetchKitInfo() {\n      const { data } = await axios.get(\"/kits/request/getKitInfo\");\n      let kitData = data.kitInfo;\n      console.log(data);\n      if (data.success) {\n        this.postcode = kitData.postcode;\n        this.address = kitData.address;\n        this.extraAddress = kitData.extraAddress;\n      } else {\n        console.log(data.message);\n      }\n    },\n  },\n  created() {\n    this.fetchInfo();\n    this.fetchKitInfo();\n    // this.fetchTestresults()\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n*:not(i):not(button):not(input[type=password])  {\n  font-family: $font-main, sans-serif !important;\n}\n\n.modal {\n  display: flex;\n  flex-direction: column;\n  z-index: 100;\n  position: fixed;\n  display: flex;\n  width: 100%;\n  height: 100%;\n  align-items: center;\n  justify-content: center;\n  \n  .close_button {\n    display: flex;\n    position: relative;\n\n    i {\n      position: absolute;\n      right: 0;\n      padding: 0;\n      font-size: 24px;\n      color: $primary;\n\n      &:hover {\n        color: $blue;\n      }\n    }\n  }\n\n \n\n  button {\n    display: flex;\n    width: 300px;\n    margin: auto;\n  }\n\n\n\n  .hr-sect {display: flex; flex-basis: 100%; align-items: center; color: rgba(0, 0, 0, 0.35); font-size: 12px; margin: 8px 0px;}\n\n  .hr-sect::before, .hr-sect::after {content: \"\"; flex-grow: 1; background: rgba(0, 0, 0, 0.35); height: 1px; font-size: 0px; line-height: 0px; margin: 0px 16px;}\n\n\n\n  &__overlay {\n    position: absolute;\n    width: 100%;\n    height: 100%;\n\n    background-color: black;\n    opacity: 0.5;\n  }\n\n  &__window {\n    align-items: center;\n    flex-direction: column;\n    justify-content: center;\n    display: block;\n    overflow: hidden;\n    width: 18rem;\n    height: 26rem;\n    border-radius: 0.4rem;\n    overflow: hidden;\n    padding: 24px;\n    z-index: 1;\n    box-shadow: 0px 4px 20px 0px $secondary;\n    background-color: white;\n  }\n\n\t// 상황에 따라 transition 변경가능 enter,leave class는 상단 문서 참고\n  &-enter,\n  &-leave-to {\n    opacity: 0;\n    transition: opacity 0.4s ease;\n  }\n\n  &-enter-to,\n  &-leave {\n    transition: opacity 0.4s ease;\n  }\n}\n</style>"],"sourceRoot":""}]}