CAAP;AACF;;AACA,eAAO,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,cAAlB,EAAkC,MAAM,CAAC,IAAP,CAAY,OAA9C,CAAP;AACD,OAdH,EAeG,KAfH,CAeS,UAAC,GAAD;AAAA,eAAS,OAAO,CAAC,GAAR,CAAY,GAAZ,CAAT;AAAA,OAfT;AAgBD,KAtEM;AAuED,IAAA,YAvEC,0BAuEc;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEb,KAAK,CAAC,GAAN,CAAU,mBAAV,EACH,IADG,CACE,UAAC,MAAD,EAAY;AAChB,kBAAA,OAAO,CAAC,GAAR,CAAY,MAAM,CAAC,IAAnB,EADgB,CAEhB;AACA;AACA;;AAFA;AACA;AACA;AACA,sBAAI,MAAK,GAAI,IAAI,UAAJ,EAAb;;AACA,kBAAA,MAAM,CAAC,MAAP,GAAgB,UAAC,CAAD,EAAO;AACrB,oBAAA,OAAO,CAAC,GAAR,CAAY,CAAC,CAAC,MAAd,EADqB,CAErB;AACA;AACD,mBAJD;;AAKA,kBAAA,MAAM,CAAC,aAAP,CAAqB,MAAI,CAAC,YAA1B;AACA,kBAAA,MAAI,CAAC,OAAL,GAAe,MAAI,CAAC,YAApB;AACA,kBAAA,OAAO,CAAC,GAAR,CAAY,MAAI,CAAC,YAAjB;AACA,kBAAA,OAAO,CAAC,GAAR,CAAY,MAAI,CAAC,OAAjB;AACD,iBAhBG,EAiBH,KAjBG,CAiBG,OAAO,CAAC,GAjBX,CAFa;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBpB;AA3FM,GAzBI;AAsHb,EAAA,OAtHa,qBAsHH;AACR,SAAK,YAAL;AACD;AAxHY,CAAf","sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"progressbar-container\">\n      <ProgressBar :percentage=\"percentage\" :step=\"step\" :testSize=\"testSize\" :currentStep=\"currentStep\" />\n    </div>\n    <!-- step 1 : kit upload -->\n    <div class=\"spinner-box\" v-if=\"isLoading\">\n      <PulseLoader :loading=\"loading\" :color=\"color\" :size=\"size\"></PulseLoader>\n    </div>\n\n    <div v-if=\"!isLoading\" class=\"item kit-upload\">\n      <div class=\"status-bar\">\n        <p>키트 업로드</p>\n      </div>\n      <div class=\"description\">\n        키트를 통해 가슴을 측정해 보시고\n        <br />\n        사진을 찍어 업로드 해주세요.\n        <br />\n        <span>양쪽 가슴의 유선이나 부피가 많이 다르다면 양쪽 모두 측정해주시는 것을 권합니다!</span>\n      </div>\n\n      <div class=\"upload-container\">\n        <div class=\"image-upload1 btn-container\">\n          <img class=\"btn-img\" src=\"@/assets/leftkit.png\" @click=\"$refs.file1.click\" />\n\n          <input type=\"file\" accept=\"image/*\" ref=\"file1\" style=\"display: none\" @change=\"changeFile1\" />\n\n          <img v-if=\"preImageLeft\" :src=\"preImageLeft\" alt=\"\" class=\"preview-img\" />\n          <i\n            @click=\"\n              preImageLeft = '';\n              leftObj = undefined;\n            \"\n            v-if=\"preImageLeft\"\n            class=\"fas fa-times-circle cancel-icon\"\n          ></i>\n        </div>\n        <div class=\"image-upload2 btn-container\">\n          <img class=\"btn-img\" src=\"@/assets/rightkit.png\" @click=\"$refs.file2.click\" />\n\n          <input type=\"file\" accept=\"image/*\" ref=\"file2\" style=\"display: none\" @change=\"changeFile2\" />\n\n          <img v-if=\"preImageRight\" :src=\"preImageRight\" alt=\"\" class=\"preview-img\" />\n          <i\n            @click=\"\n              preImageRight = '';\n              rightObj = undefined;\n            \"\n            v-if=\"preImageRight\"\n            class=\"fas fa-times-circle cancel-icon\"\n          ></i>\n        </div>\n      </div>\n      <button @click=\"submitKit\" type=\"button\" class=\"btn-primary btn-55 btn-upload\">키트 업로드</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProgressBar from \"@/components/ProgressBar.vue\";\nimport PulseLoader from \"vue-spinner/src/PulseLoader.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"Test\",\n  data() {\n    return {\n      step: 1,\n      isLoading: false,\n      color: \"#FFD07F\",\n      size: \"1em\",\n      image: \"\",\n      preImageLeft: \"\",\n      preImageRight: \"\",\n      leftObj: {},\n      rightObj: {},\n      testSize: 20,\n      currentStep: 0,\n      percentage: 0,\n    };\n  },\n  components: {\n    ProgressBar,\n    PulseLoader,\n  },\n  mounted() {\n    window.scrollTo(0, 0);\n  },\n  methods: {\n    add() {\n      this.currentStep++;\n    },\n    minus() {\n      this.currentStep--;\n    },\n    changeFile1(e) {\n      let input = e.target;\n      if (input.files && input.files[0]) {\n        let reader = new FileReader();\n        reader.onload = (e) => {\n          console.log(e.target);\n          this.preImageLeft = e.target.result;\n          // console.log(e.target.result)\n        };\n        reader.readAsDataURL(input.files[0]);\n        this.leftObj = input.files[0];\n        console.log(input.files[0]);\n        console.log(this.leftObj);\n      }\n    },\n    changeFile2(e) {\n      let input = e.target;\n      if (input.files && input.files[0]) {\n        let reader = new FileReader();\n        reader.onload = (e) => {\n          this.preImageRight = e.target.result;\n          // console.log(e.target.result)\n        };\n        reader.readAsDataURL(input.files[0]);\n        this.rightObj = input.files[0];\n      }\n    },\n    submitKit() {\n      if (!this.preImageLeft && !this.preImageRight) {\n        this.isLoading = false;\n        this.emitter.emit(\"showRedToast\", \"사진을 업로드해주세요\");\n        return;\n      }\n      // if (!this.leftObj && !this.rightObj) {\n      //   alert(\"No Image!\");\n      //   console.log(\"No Image!\");\n      // }\n      let fd = new FormData();\n      let data = {\n        userdata: this.$store.state.usercode,\n      };\n      let dataString = JSON.stringify(data);\n      console.log(dataString);\n      fd.append(\"left\", this.leftObj);\n      fd.append(\"right\", this.rightObj);\n      fd.append(\"data\", dataString);\n      this.isLoading = true;\n      axios.post(\"/kits/img/upload\", fd, { headers: {\"Content-Type\": \"multipart/form-data\" }})\n        .then((result) => {\n          console.log(result.data);\n          //이미지 업로드 결과 : 정상 -> 다음 단계로 (로딩페이지 거쳐서) , 비정상 -> 재업로드 요청\n          this.isLoading = false;\n          if (result.data.success) {\n            //state 저장 로직 추가\n            console.log(result.data.message);\n            // 해당 유저가 이전에 마무리한 설문 페이지받아서 이동\n            // const lastPage = result.data.lastPage\n            // this.$router.push(`/survey/${lastPage}`)\n            return this.$router.push(\"/survey/2\");\n          }\n          return this.emitter.emit(\"showRedToast\", result.data.message);\n        })\n        .catch((err) => console.log(err));\n    },\n    async fetchKitInfo() {\n      // const { data } = await fetchUserKitData()\n      await axios.get(\"/kits/img/getPath\")\n        .then((result) => {\n          console.log(result.data);\n          // this.preImageLeft = result.data.leftImgPath && '../../'+(result.data.leftImgPath);\n          // this.preImageRight = result.data.rigtImgPath && '../../'+(result.data.rigtImgPath);\n          // console.log(this.preImageLeft, this.preImageRight);\n          let reader = new FileReader();\n          reader.onload = (e) => {\n            console.log(e.target);\n            // this.preImageLeft = e.target.result;\n            // console.log(e.target.result)\n          };\n          reader.readAsDataURL(this.preImageLeft);\n          this.leftObj = this.preImageLeft;\n          console.log(this.preImageLeft);\n          console.log(this.leftObj);\n        })\n        .catch(console.log);\n    },\n  },\n  created() {\n    this.fetchKitInfo();\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n  height: 140vh;\n  background-color: $white;\n  display: flex-start;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 100px 100px;\n\n  @media (max-width: 768px) {\n    padding: 90px 55px;\n    height: 1400px;\n  }\n\n  .upload-container {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n\n    .image-upload1 {\n      margin-right: 16px;\n    }\n  }\n\n  .btn-container {\n    position: relative;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 200px;\n    height: 200px;\n    border-radius: 15px;\n    background-color: $background;\n    margin-bottom: 32px;\n    overflow: hidden;\n    box-shadow: 0px 3px 7px 0px $tertiary;\n\n    @media (max-width: 768px) {\n      width: 150px;\n      height: 150px;\n    }\n\n    .preview-img {\n      position: absolute;\n      top: 1px;\n      left: 1px;\n      right: 1px;\n      display: block;\n      height: 100%;\n      width: 100%;\n      border-radius: 15px;\n\n      &:hover {\n        opacity: 0.5;\n      }\n    }\n\n    i {\n      position: absolute;\n      right: 5%;\n      top: 5%;\n      /* left: 50%;\n      bottom: 50%; */\n      z-index: 50;\n      font-size: 2rem;\n      color: $secondary;\n      cursor: pointer;\n      z-index: 3;\n\n      &:hover {\n        opacity: 0.5;\n      }\n    }\n  }\n\n  .item {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n  }\n\n  .description {\n    @include text-style(16, $primary);\n    text-align: center;\n    margin: 50px;\n\n    span {\n      @include text-style(14, $secondary);\n    }\n\n    @media (max-width: 768px) {\n      margin: 20px;\n    }\n  }\n\n  /* imaged preview */\n  .filebox .upload-display {\n    /* 이미지가 표시될 지역 */\n    margin-bottom: 5px;\n  }\n\n  @media (min-width: 768px) {\n    .filebox .upload-display {\n      display: inline-block;\n      margin-right: 5px;\n      margin-bottom: 0;\n    }\n  }\n  .filebox .upload-thumb-wrap {\n    /* 추가될 이미지를 감싸는 요소 */\n    display: inline-block;\n    width: 54px;\n    padding: 2px;\n    vertical-align: middle;\n    border: 1px solid #ddd;\n    border-radius: 5px;\n    background-color: #fff;\n  }\n\n  .filebox .upload-display img {\n    /* 추가될 이미지 */\n    display: block;\n    max-width: 100%;\n    width: 100% \\9;\n    height: auto;\n  }\n\n  .file-left label {\n    @include text-style(16);\n    display: inline-flexbox;\n    justify-content: center;\n    align-items: center;\n\n    padding: 16px 8px;\n    color: #fff;\n    font-weight: 700;\n    /* line-height: normal; */\n    /* vertical-align: middle; */\n    background-color: $blue;\n    cursor: pointer;\n    /* border: 1px solid #4cae4c; */\n    border-radius: 4px;\n    -webkit-transition: background-color 0.2s;\n    transition: background-color 0.2s ease-in-out;\n  }\n\n  .file-left label:not(:disabled):hover {\n    background-color: $blue-dark;\n  }\n\n  .file-left input[type=\"file\"] {\n    position: absolute;\n    width: 1px;\n    height: 1px;\n    padding: 0;\n    margin: -1px;\n    overflow: hidden;\n    clip: rect(0, 0, 0, 0);\n    border: 0;\n  }\n  .preview-img {\n    width: 80px;\n    height: 80px;\n  }\n\n  .btn-img {\n    &:hover {\n      opacity: 0.7;\n    }\n\n    @media (max-width: 768px) {\n      width: 75px;\n      height: 75px;\n    }\n  }\n\n  .btn-upload {\n    margin-top: 32px;\n    width: 150px;\n  }\n\n  .spinner-box {\n    display: flex;\n    margin-top: 240px;\n    align-items: center;\n    justify-content: center;\n  }\n\n  .status-bar {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 50px;\n\n    p {\n      @include text-style(18, $primary);\n      text-align: center;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}