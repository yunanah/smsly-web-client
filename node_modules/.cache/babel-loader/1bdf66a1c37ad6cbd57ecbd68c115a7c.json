OAAL,CAAa,IAAb,CAAkB,cAAlB,EAAkC,MAAM,CAAC,IAAP,CAAY,OAA9C;AACF;AACD,iBAbH,EAcG,KAdH,CAcS,OAAO,CAAC,GAdjB;;AAZiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2BlB,KA9GM;AA+GD,IAAA,SA/GC,uBA+GW;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACV,KAAK,CAAC,GAAN,CAAU,0BAAV,EACH,IADG,CACE,UAAC,MAAD,EAAY;AAChB,kBAAA,OAAO,CAAC,GAAR,CAAY,MAAM,CAAC,IAAnB;;AACA,sBAAI,MAAM,CAAC,IAAP,CAAY,OAAhB,EAAyB;AACvB,oBAAA,OAAO,CAAC,GAAR,CAAY,MAAM,CAAC,IAAP,CAAY,OAAxB;AACA,wBAAI,IAAG,GAAI,MAAM,CAAC,IAAP,CAAY,OAAvB;AACA,oBAAA,MAAI,CAAC,QAAL,GAAgB,IAAI,CAAC,QAArB;AACA,oBAAA,MAAI,CAAC,KAAL,GAAa,IAAI,CAAC,KAAlB;AACA,oBAAA,MAAI,CAAC,KAAL,GAAa,IAAI,CAAC,KAAlB;AACA,oBAAA,MAAI,CAAC,QAAL,GAAgB,IAAI,CAAC,QAArB;AACA,oBAAA,MAAI,CAAC,OAAL,GAAe,IAAI,CAAC,OAApB;AACA,oBAAA,MAAI,CAAC,YAAL,GAAoB,IAAI,CAAC,YAAzB;AACA,oBAAA,MAAI,CAAC,OAAL,GAAe,IAAI,CAAC,OAApB;AACF;AACD,iBAdG,EAeH,KAfG,CAeG,OAAO,CAAC,GAfX,CADU;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBjB;AAhIM,GAlBI;AAoJb,EAAA,OApJa,qBAoJH;AACR,SAAK,SAAL;AACD;AAtJY,CAAf","sourcesContent":["<template>\n  <div class=\"body\">\n    <form @submit.prevent=\"submitForm\">\n      <h2>키트 신청하기</h2>\n      <p>\n        {{ username }}님 무료로 키트를 배송 받을 주소를 입력해 주세요.\n        <br />\n        상세주소까지 입력해주셔야 분실되지 않아요.\n      </p>\n\n      <div class=\"postcode-container\">\n        <div class=\"input-group\">\n          <input class=\"form-input postcode\" type=\"text\" placeholder=\"우편번호\" v-model=\"postcode\" />\n        </div>\n\n        <button type=\"button\" class=\"btn-primary btn-40\" @click=\"execDaumPostcode\">주소 검색</button>\n      </div>\n      <br />\n      <div\n        ref=\"searchWindow\"\n        :style=\"searchWindow\"\n        style=\"border: 1px solid; width: 100%; height: 350px; margin: 5px 0; position: relative; margin-bottom: 16px\"\n      >\n        <img\n          src=\"//t1.daumcdn.net/postcode/resource/images/close.png\"\n          id=\"btnFoldWrap\"\n          style=\"cursor: pointer; position: absolute; right: 0px; top: -1px; z-index: 1\"\n          @click=\"searchWindow.display = 'none'\"\n          alt=\"close\"\n        />\n      </div>\n      <div class=\"input-group\">\n        <input class=\"form-input\" type=\"text\" v-model=\"address\" placeholder=\"주소\" />\n      </div>\n      <br />\n      <div class=\"input-group\">\n        <input class=\"form-input\" type=\"text\" v-model=\"extraAddress\" ref=\"extraAddress\" placeholder=\"상세주소\" />\n      </div>\n      <div class=\"input-group\">\n        <input style=\"margin-top: 28px\" class=\"form-input\" type=\"text\" v-model=\"message\" placeholder=\"배송 요청사항\" />\n      </div>\n\n      <div class=\"input-group\">\n        <label for=\"phone\">키트가 발송되면 메세지를 보내드려요!</label>\n        <input style=\"margin-bottom: 16px\" type=\"text\" class=\"form-input\" v-model=\"username\" placeholder=\"받으실 분의 이름을 입력해주세요.\" />\n        <input\n          class=\"form-input\"\n          type=\"text\"\n          v-model=\"phone\"\n          @keyup=\"getPhoneMask(phone)\"\n          ref=\"extraAddress\"\n          placeholder=\"받으실 분의 휴대폰 번호를 입력해주세요.\"\n        />\n      </div>\n\n      <button v-bind:disabled=\"extraAddress === '' || phone === ''\" type=\"submit\" class=\"btn-outlined btn-40 order-btn\">키트 신청하기</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n// import { fetchUserData } from '@/api/index'\n\nexport default {\n  data() {\n    return {\n      searchWindow: {\n        display: \"none\",\n        height: \"300px\",\n      },\n      postcode: \"\",\n      address: \"\",\n      extraAddress: \"\",\n      phone: \"\",\n      message: \"\",\n      username: \"\",\n    };\n  },\n  mounted() {\n    window.scrollTo(0, 0);\n  },\n  methods: {\n    execDaumPostcode() {\n      const currentScroll = Math.max(document.body.scrollTop, document.documentElement.scrollTop);\n      console.log(\"what\");\n      // eslint-disable-next-line\n      new window.daum.Postcode({\n        onComplete: (data) => {\n          if (data.userSelectedType === \"R\") {\n            this.address = data.roadAddress;\n          } else {\n            this.address = data.jibunAddress;\n          }\n          if (data.userSelectedType === \"R\") {\n            if (data.bname !== \"\" && /[동|로|가]$/g.test(data.bname)) {\n              this.extraAddress += data.bname;\n            }\n            if (data.buildingName !== \"\" && data.apartment === \"Y\") {\n              this.extraAddress += this.extraAddress !== \"\" ? `, ${data.buildingName}` : data.buildingName;\n            }\n            if (this.extraAddress !== \"\") {\n              this.extraAddress = ` (${this.extraAddress})`;\n            }\n          } else {\n            this.extraAddress = \"\";\n          }\n          this.postcode = data.zonecode;\n          this.$refs.extraAddress.focus();\n          this.searchWindow.display = \"none\";\n          document.body.scrollTop = currentScroll;\n        },\n        onResize: (size) => {\n          this.searchWindow.height = `${size.height}px`;\n        },\n        width: \"100%\",\n        height: \"100%\",\n      }).embed(this.$refs.searchWindow);\n      this.searchWindow.display = \"block\";\n    },\n    getPhoneMask(val) {\n      let res = this.getMask(val);\n      this.phone = res;\n      //서버 전송 값에는 '-' 를 제외하고 숫자만 저장\n      // this.model.contact = this.contact.replace(/[^0-9]/g, '')\n    },\n\n    getMask(phoneNumber) {\n      if (!phoneNumber) return phoneNumber;\n      phoneNumber = phoneNumber.replace(/[^0-9]/g, \"\");\n\n      let res = \"\";\n      if (phoneNumber.length < 3) {\n        res = phoneNumber;\n      } else {\n        if (phoneNumber.substr(0, 2) == \"02\") {\n          if (phoneNumber.length <= 5) {\n            //02-123-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 3);\n          } else if (phoneNumber.length > 5 && phoneNumber.length <= 9) {\n            //02-123-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 3) + \"-\" + phoneNumber.substr(5);\n          } else if (phoneNumber.length > 9) {\n            //02-1234-5678\n            res = phoneNumber.substr(0, 2) + \"-\" + phoneNumber.substr(2, 4) + \"-\" + phoneNumber.substr(6);\n          }\n        } else {\n          if (phoneNumber.length < 8) {\n            res = phoneNumber;\n          } else if (phoneNumber.length == 8) {\n            res = phoneNumber.substr(0, 4) + \"-\" + phoneNumber.substr(4);\n          } else if (phoneNumber.length == 9) {\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 3) + \"-\" + phoneNumber.substr(6);\n          } else if (phoneNumber.length == 10) {\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 3) + \"-\" + phoneNumber.substr(6);\n          } else if (phoneNumber.length > 10) {\n            //010-1234-5678\n            res = phoneNumber.substr(0, 3) + \"-\" + phoneNumber.substr(3, 4) + \"-\" + phoneNumber.substr(7);\n          }\n        }\n      }\n\n      return res;\n    },\n\n    async submitForm() {\n      //서버에서 동일하게 데이터 keys 목록 받는지 체크\n      const kitData = {\n        usercode: this.$store.state.usercode,\n        // email: this.$store.state.email,\n        recipient: this.username,\n        postcode: this.postcode,\n        address: this.address,\n        extraAddress: this.extraAddress,\n        message: this.message,\n        phone: this.phone,\n      };\n      axios.post(\"/kits/request\", kitData)\n        .then((result) => {\n          console.log(result.data);\n          if (result.data.success) {\n            console.log(result.data.message);\n            this.emitter.emit(\"showToast\", result.data.message);\n            setTimeout(() => {\n              this.$router.push(\"/\");\n            }, 3000);\n          } else {\n            console.log(result.data.message);\n            this.emitter.emit(\"showRedToast\", result.data.message);\n          }\n        })\n        .catch(console.log);\n    },\n    async fetchInfo() {\n      await axios.get(\"/kits/request/getKitInfo\")\n        .then((result) => {\n          console.log(result.data);\n          if (result.data.success) {\n            console.log(result.data.message);\n            let data = result.data.kitInfo;\n            this.username = data.username;\n            this.email = data.email;\n            this.phone = data.phone;\n            this.postcode = data.postcode;\n            this.address = data.address;\n            this.extraAddress = data.extraAddress;\n            this.message = data.message;\n          }\n        })\n        .catch(console.log);\n    },\n  },\n  created() {\n    this.fetchInfo();\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n  display: flex;\n  /* flex-wrap: wrap; */\n  background-color: $gray;\n  justify-content: center;\n  align-items: flex-start;\n  height: 1200px;\n  width: 100%;\n  margin: 0 auto;\n  \n\n  @media screen and (max-width: $md-breakpoint - 1px) {\n    height: 1200px;\n  }\n\n  form {\n    padding: 24px;\n    width: 50%;\n    display: flex;\n    border-radius: 8px;\n    box-shadow: 1px 4px 4px 1px $border;    \n    flex-direction: column;\n    background-color: $white;\n    width: 500px;\n    margin-top: 96px;\n    @media screen and (max-width: $md-breakpoint - 1px) {\n      width: 95%;\n    }\n    /* width: 90%; */\n    /* @include responsive(); */\n      /* @include responsive(); */ \n    /* @include responsive(); */\n\n    h2 {\n      @include text-style(24, $primary);\n      margin-top: 16px;\n      margin-bottom: 32px;\n    }\n\n    p {\n      @include text-style(16, $secondary);\n\n      @media screen and (max-width: $md-breakpoint - 1px) {\n        @include text-style(14);\n      }\n    }\n  }\n\n  .input-group {\n    /* margin: 24px 0; */\n\n    label {\n      @include text-style(13, $primary);\n      display: inline-block;\n      /* font-weight: bold; */\n      margin-top: 32px;\n      margin-bottom: 4px;\n      color: $green;\n    }\n  }\n\n  .postcode-container {\n    margin-top: 24px;\n    display: flex;\n    /* flex-direction: row; */\n    /* justify-content: space-around; */\n\n    button {\n      /* flex-grow: 1; */\n      margin-left: 16px;\n      width: 130px;\n    }\n  }\n\n  .postcode {\n    width: 200px;\n  }\n\n  .order-btn {\n    margin-top: 32px;\n    /* margin-bottom: 600px; */\n  }\n}\n</style>\n"],"sourceRoot":""}]}