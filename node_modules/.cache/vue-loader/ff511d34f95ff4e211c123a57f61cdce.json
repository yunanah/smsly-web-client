rcle\" :to=\"{name: 'KitOrder'}\" > -->\n            <div class=\"circle\" @click=\"checkState\">\n              <span>{{ buttonStep }}</span>\n              <p>{{ buttonContent }}</p>\n            </div>\n            <!-- </router-link> -->\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"how-to\">\n      <div data-aos=\"fade-up\" data-aos-anchor-placement=\"top-bottom\" data-aos-duration=\"1000\"  class=\"how-to-use\">\n        <h1>How to</h1>\n        <div class=\"buttons\">\n          <button @click=\"step = 1\" :class=\"{ selected: step == 1 }\">STEP1</button>\n          <button @click=\"step = 2\" :class=\"{ selected: step == 2 }\">STEP2</button>\n          <button @click=\"step = 3\" :class=\"{ selected: step == 3 }\">STEP3</button>\n          <button @click=\"step = 4\" :class=\"{ selected: step == 4 }\">STEP4</button>\n        </div>\n        <div class=\"img-step\">\n          <img v-if=\"step == 1\" src=\"@/assets/step1.png\" alt=\"step1 입니다.\" />\n          <img v-if=\"step == 2\" src=\"@/assets/step2.png\" alt=\"step2 입니다.\" />\n          <img v-if=\"step == 3\" src=\"@/assets/step3.png\" alt=\"step3 입니다.\" />\n          <img v-if=\"step == 4\" src=\"@/assets/step4.png\" alt=\"step4 입니다.\" />\n        </div>\n      </div>\n      <div class=\"how-understand\">\n        <!-- <h1>Story</h1> -->\n        <div>\n          <button type=\"button\" class=\"btn-primary btn-32\">버튼입니다</button>\n          <button type=\"button\" class=\"btn-secondary btn-32\">버튼입니다</button>\n          <button type=\"button\" class=\"btn-outlined btn-32\">버튼입니다</button>\n          <button type=\"button\" class=\"btn-ghost btn-32\">버튼입니다</button>\n        </div>\n        <div>\n          <button type=\"button\" class=\"btn-primary btn-40\">버튼입니다</button>\n          <button type=\"button\" class=\"btn-secondary btn-40\">버튼입니다</button>\n          <button type=\"button\" class=\"btn-outlined btn-40\">버튼입니다</button>\n          <button type=\"button\" class