breastTest/save/15\", answers)\n        .then((result) => {\n          console.log(result.data);\n          this.$router.push(\"/survey/14\");\n        })\n        .catch(console.log);\n    },\n    async goNext() {\n      if (this.answer1 === null || this.dateBirth === null) {\n        return this.emitter.emit(\"showRedToast\", \"답변을 입력해주세요\");\n      }\n      let answers = {\n        PK_ID: this.$store.state.PK_ID,\n        [this.page[0].column] : this.answer1,\n        [this.page[1].column] : this.dateBirth\n      };\n      await axios.post(\"/breastTest/save/15\", answers)\n        .then((result) => {\n          console.log(result.data);\n          this.fetchAnswerInfo();\n          // this.$router.push(\"/survey/complete\");\n        })\n        .catch(console.log);\n        this.endModal();\n    },\n    async fetchAnswerInfo() {\n      axios.get(\"/breastTest/getData/15\")\n        .then((result) => {\n          console.log(result.data);\n          if (result.data.success) {\n            const ans1 = result.data.record[this.page[0].column];\n            if (ans1 !== null) this.setAnswer(ans1);\n\n            const ans2 = result.data.record[this.page[1].column];\n            if (ans2 !== null) {\n              this.selections[0] = String(ans2);\n              this.dateBirth = ans2;\n            }\n          }\n        })\n        .catch(console.log);\n    },\n    async fetchProgress() {\n      axios.get(\"/breastTest/getProgress\")\n        .then((result) => {\n          console.log(result.data);\n          this.percentage = result.data.Progress;\n        })\n        .catch(console.log);\n    },\n    endModal() {\n      this.emitter.emit('EndupTestModal', true)\n    }\n  },\n  created() {\n    this.fetchAnswerInfo();\n    this.fetchProgress();\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n  height: 160vh;\n  background-color: $gray;\n  display: flex-start;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n  padd