            password: '',\n            passwordCheck: '',\n        }\n    },\n    methods: {\n        async setPassword() {\n            let data = {\n                token: this.$route.query.token,\n                newPassword: this.password\n            }\n            console.log(data);\n            axios.post('users/editPwd/setNew', data)\n            .then((res) => {\n                if(res.data.success) {\n                    console.log(res)\n                    let message = \"비밀번호가 변경되었습니다.\"\n                    this.emitter.emit('showToast', message)\n                    this.$router.push('/')\n                }\n                console.log(res);\n            })\n            .catch(console.log)\n        }\n    },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.body {\n    /* z-index: 110; */\n    /* margin: 20px; */\n    height: 80vh;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n\n    h3 {\n        @include text-style(18, $primary);\n        margin-bottom: 8px;\n    }\n\n    .input-group {\n        margin: 8px 0;\n        width: 300px;\n    } \n\n    button {\n        margin-top: 8px;\n        width: 300px;\n    }\n\n    p {\n        @include text-style(14, $red);\n    }\n}\n</style>"]}]}